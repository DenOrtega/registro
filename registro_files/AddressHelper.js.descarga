	    	if(currSearchTerm.length > 0){
		        dojo.cookie('searchStreetValue', dojo.trim(this.findElementsForMapCity(elementName).value), {path:'/'});
		        return true;
	        }else{
	        	return false;
	        }
	    }else{
	    	
	    	return e.keyCode!=13;
	    }
	},
